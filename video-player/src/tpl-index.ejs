          content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <% for(var i = 0; i < htmlWebpackPlugin.files.css; i++) { %>
